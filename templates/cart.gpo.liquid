{% layout none %}
[
  {%- for item in cart.items -%}
    {
      "id": {{ item.id }},
      "title": {{ item.title | json }},
      "collection": {{ item.product.collections | map: 'id' | json }},
      "tags": {{ item.product.tags | json }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]